<Window x:Class="PCSX2_Configurator.Frontend.Wpf.ConfigWizard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Create Config" Width="250" Height="180">
    <Grid>
        <Label Content="Config Name:" HorizontalAlignment="Center" VerticalAlignment="Top" Width="120" Margin="0,2,0,0"/>
        <TextBox Name="configName" HorizontalAlignment="Center" Margin="0,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" TextAlignment="Center"/>
        <Label Content="Version:" HorizontalAlignment="Center" VerticalAlignment="Top" Width="120" Margin="0,51,0,0"/>
        <ComboBox Name="version" HorizontalAlignment="Center" VerticalAlignment="Top" Width="120" Margin="0,77,0,0"/>
        <Button Content="Go" HorizontalAlignment="Center" Margin="0,114,0,0" VerticalAlignment="Top" Width="50" Click="CreateConfig"/>
    </Grid>
</Window>
